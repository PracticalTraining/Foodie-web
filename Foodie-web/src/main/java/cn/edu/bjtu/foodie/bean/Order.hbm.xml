<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.bjtu.foodie.bean.Order" table="Foodie_order">
		<id name="id" column="id">
			<generator class="guid" />
		</id>
		<property name="dishIds"></property>
		<property name="foodieId"></property>
	</class>
</hibernate-mapping>  